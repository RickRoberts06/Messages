<h1>Edit your message </h1>

<%= form_for @message do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :department %>
  <%= f.text_field :department %><br>
  <%= f.label :message %>
  <%= f.text_field :message %><br>
  <%= f.submit %>
<% end %>
